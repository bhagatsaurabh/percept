    "canvas rendering library",
    "rendering library"
  ],
  "author": "Saurabh Bhagat",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.22",
    "@types/offscreencanvas": "^2019.6.4",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^8.0.14",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0"
  }
