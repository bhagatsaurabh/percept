{
  "name": "canvas-percept",
  "version": "1.0.8",
  "description": "HTML5 Canvas rendering engine",
  "main": "lib/percept.js",
  "types": "lib/percept.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "clean": "shx rm -rf bundles lib lib-esm",
